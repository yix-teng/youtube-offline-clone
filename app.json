{
  "expo": {
    "name": "YouTube",
    "slug": "YouTube",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/youtube-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/youtube_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yix.teng.youtube"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/youtube-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.yix.teng.youtube"
    },
    "web": {
      "favicon": "./assets/youtube-icon.png"
    }
  }
}
